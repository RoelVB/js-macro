{
  "name": "js-macro",
  "version": "1.1.2",
  "description": "A npm package that lets you automate your windows desktop.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "os": [
    "win32"
  ],
  "scripts": {
    "install": "node-gyp rebuild && npm run test && npx tsc --project tsconfig.json",
    "prepare": "npx prettier --write src/**/*.ts",
    "test": "npx eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vierofernando/js-macro.git"
  },
  "keywords": [
    "automation",
    "win32",
    "windows",
    "macros"
  ],
  "author": {
    "name": "vierofernando",
    "email": "vierofernando9@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vierofernando/js-macro/issues",
    "email": "vierofernando9@gmail.com"
  },
  "homepage": "https://github.com/vierofernando/js-macro#readme",
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "node-gyp": "^9.0.0",
    "pre-commit": "1.2.2",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "pre-commit": {
    "run": [
      "prepare"
    ],
    "silent": true
  },
  "type": "module"
}
